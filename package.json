{
  "name": "react-router-startup",
  "description": "A Startup Devlopment Kit for React Router",
  "version": "1.0.0",
  "dependencies": {
    "alt": "^0.18.4",
    "alt-utils": "^1.0.0",
    "express": "^4.13.4",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-router": "^2.4.1",
    "swig": "^1.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-watch": "^2.0.2",
    "babelify": "^7.3.0",
    "bower": "^1.7.9",
    "browserify": "^13.0.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-cssmin": "^0.1.7",
    "gulp-if": "^2.0.1",
    "gulp-less": "^3.1.0",
    "gulp-plumber": "^1.1.0",
    "gulp-streamify": "^1.0.2",
    "gulp-uglify": "^1.5.3",
    "nodemon": "^1.9.2",
    "underscore": "^1.8.3",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "dev": "gulp & nodemon app.js --exec babel-node --presets es2015",
    "start": "node dist/app.js",
    "build": "rm -rf dist && gulp build && npm run build-js && npm run build-main && npm run copy-res",
    "copy-res": "cp -rf public dist",
    "build-js": "babel src --out-dir dist/src",
    "build-main": "babel app.js -o dist/app.js"
  }
}
